<?xml version="1.0" encoding="utf-8" ?>
<!--数据库配置文件示例。-->
<!--DataBaseConfig为数据库配置文件根节点名称-->
<DataBaseConfig>
  
  <!--
  NodeName为节点名称，用来区分多个数据库配置。
  databaseType为数据库类型，如果没配置默认为MSSQL。
  -->
  <NodeName databaseType="MSSQL">
    <!--根据不同类型数据库设置配置文件。-->
  </NodeName>

  <!--
  SQLServer数据库配置示例。
  -->
  <SQLServer databaseType="MSSQL">
    <!--服务器-->
    <ServerName>localhost</ServerName>
    <!--数据库端口号，如果不配置则问默认端口-->
    <ServerPort>1433</ServerPort>
    <!--数据库实例名，如果不配置则使用默认实例名-->
    <Example></Example>
    <!--数据库名称-->
    <DataBaseName>jsw</DataBaseName>
    <!--登陆用户名-->
    <UserID>sa</UserID>
    <!--登陆密码-->
    <UserPassword>123456</UserPassword>
  </SQLServer>
</DataBaseConfig>